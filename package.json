{
  "name": "react-paint",
  "version": "1.0.1",
  "description": "Boilerplate for creating React Npm packages with ES2015",
  "repository": {
    "type": "git",
    "url": "https://github.com/james-oldfield/react-paint.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/james-oldfield/react-paint/issues"
  },
  "homepage": "https://github.com/james-oldfield/react-paint",
  "author": "james oldfield",
  "keywords": [
    "react",
    "canvas",
    "canvas draw",
    "paint",
    "react-component"
  ],
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepublish": "babel --plugins 'transform-es2015-modules-umd' src --ignore __tests__ --out-dir ./dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "9.1.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^4.3.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "react": "18.2.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "18.2.0",
    "sinon": "^1.17.3"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "react": "18.2.0"
  }
}
